<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="a.a.component2"
    android:sharedUserId="component.dev.id"
    >

    <application
        android:name="debug.C2Application"
        android:allowBackup="true"
        android:icon="@mipmap/c2_ic_launcher"
        android:label="@string/c2_app_name"
        android:roundIcon="@mipmap/c2_ic_launcher_round"
        android:supportsRtl="true">

        <activity android:name=".C2MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

    </application>

</manifest>